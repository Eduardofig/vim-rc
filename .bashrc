# .bashrc

# Source global definitions
if [ -f /etc/bashrc ]; then
	. /etc/bashrc
fi

# User specific environment
if ! [[ "$PATH" =~ "$HOME/.local/bin:$HOME/bin:" ]]
then
    PATH="$HOME/.local/bin:$HOME/bin:$PATH"
fi
export PATH
alias vim='vimx'
alias vi='vimx'
alias vf='vifm'
alias q='exit'
alias nv='nvim'
alias c='clear'
alias b='byobu'
alias edex-ui='cd ~/edex-ui && npm start '
alias ydl='youtube-dl'
alias xo='xdg-open'

